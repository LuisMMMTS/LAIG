//especular mais baixa para o que nao queremos metalico
         requires a distinct "id". At least one material should be present. -->
   <material id="Porcelain_Grey">     <!--cor das paredes-->                             
       <shininess value="1" />
       <specular r="0.92" g="0.94" b="0.94" a="1" />        <!-- specular reflection -->
        <diffuse r="0.92" g="0.94" b="0.94" a="1" />         <!-- diffuse reflection -->
        <ambient r="0.92" g="0.94" b="0.94" a="1" />         <!-- ambient reflection -->
        <emissive r="0.92" g="0.94" b="0.94" a="1" />        <!-- emissive component -->
     </material>
        <material id="Off_white">        <!--cilindros dos tapetes-->                          
       <shininess value="1" />
       <specular r="0.97" g="0.97" b="1.0" a="1.0" />        <!-- specular reflection -->
        <diffuse r="0.97" g="0.97" b="1.0" a="1.0" />         <!-- diffuse reflection -->
